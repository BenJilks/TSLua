{
    "compilerOptions":
    {
        "module": "commonjs",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "target": "es2017",

        "noImplicitAny": true,
        "removeComments": true,
        "allowUnreachableCode": false,
        "strictNullChecks": true,
        "incremental": true
    },
    
    "lib":
    [
        "es2017"
    ]
}

